{
    "name": "toro/widget-bundle",
    "type": "symfony-bundle",
    "description": "Widget for Symfony2 projects.",
    "keywords": ["widget"],
    "homepage": "https://github.com/intbizth/toro-widget-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "phpspec/phpspec": "^3.1",
        "phpunit/phpunit": "^5.6",
        "twig/twig": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Toro\\Bundle\\WidgetBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Toro\\Bundle\\WidgetBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
